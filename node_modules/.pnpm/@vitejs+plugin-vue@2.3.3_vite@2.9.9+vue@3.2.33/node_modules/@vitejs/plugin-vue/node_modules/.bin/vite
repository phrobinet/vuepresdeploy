#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules/@vitejs/plugin-vue/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules/@vitejs/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules/@vitejs/plugin-vue/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules/@vitejs/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/@vitejs+plugin-vue@2.3.3_vite@2.9.9+vue@3.2.33/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.9.9/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.9.9/node_modules/vite/bin/vite.js" "$@"
fi
