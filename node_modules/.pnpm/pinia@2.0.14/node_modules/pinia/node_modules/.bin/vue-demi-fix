#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules/vue-demi/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules/vue-demi/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/pinia@2.0.14/node_modules/pinia/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/pinia@2.0.14/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules/vue-demi/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules/vue-demi/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vue-demi@0.12.5/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/pinia@2.0.14/node_modules/pinia/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/pinia@2.0.14/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vue-demi@0.12.5/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../../../../vue-demi@0.12.5/node_modules/vue-demi/bin/vue-demi-fix.js" "$@"
fi
