#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/dist/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/dist/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules/rollup/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/rollup@2.73.0/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules:/mnt/c/Users/phrob/Documents/node_modules:/mnt/c/Users/phrob/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules/vite/node_modules:/mnt/c/Users/phrob/Documents/Pinia Docs Fr/node_modules/.pnpm/vite@2.9.9/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.73.0/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.73.0/node_modules/rollup/dist/bin/rollup" "$@"
fi
